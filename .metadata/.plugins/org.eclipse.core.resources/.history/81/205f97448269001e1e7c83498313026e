#include "stm32g0xx.h"

#define GPIOAEN	(1U<<0)		// enable port A
#define PIN5	(1U<<5)
#define LED_PIN	PIN5



int main(void)
{

	// enable clock access for GPIOA
	RCC->IOPENR |= GPIOAEN;

	// set GPIOA5 as output
	GPIOA->MODER |= (1U<<10);
	GPIOA->MODER &= ~(1U<<11);


    /* Loop forever */
	for(;;){
		GPIOA->ODR ^= LED_PIN;
		for(int i=0; i < 100000; i++);
	}
}
